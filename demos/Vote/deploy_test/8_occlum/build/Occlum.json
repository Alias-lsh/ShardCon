{
  "resource_limits": {
    "user_space_size": "300MB"
  },
  "process": {
    "default_stack_size": "4MB",
    "default_heap_size": "32MB",
    "default_mmap_size": "100MB"
  },
  "entry_points": [
    "/bin"
  ],
  "env": {
    "default": [
      "OCCLUM=yes"
    ],
    "untrusted": [
      "EXAMPLE"
    ]
  },
  "mount": [
    {
      "options": {
        "layers": [
          {
            "options": {
              "MAC": "b0-e7-62-91-64-84-8e-13-66-1a-93-e7-a4-75-bc-d7"
            },
            "source": "./build/mount/__ROOT",
            "target": "/",
            "type": "sefs"
          },
          {
            "source": "./run/mount/__ROOT",
            "target": "/",
            "type": "sefs"
          }
        ]
      },
      "target": "/",
      "type": "unionfs"
    },
    {
      "source": ".",
      "target": "/host",
      "type": "hostfs"
    },
    {
      "options": {
        "temporary": true
      },
      "source": "./run/mount/tmp",
      "target": "/tmp",
      "type": "sefs"
    },
    {
      "target": "/proc",
      "type": "procfs"
    },
    {
      "target": "/dev",
      "type": "devfs"
    }
  ]
}