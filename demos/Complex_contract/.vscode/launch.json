    "version": "0.2.0",
    "configurations": [
        {
            "name": "appteeprovider1",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}/bin",
            "program": "${workspaceFolder}/bin/appteeprovider1",
            "stopAtEntry": false,
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/opt/intel/sgxsdk/bin/sgx-gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}